<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="323.0" prefWidth="378.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SelectNodeMoveController">
   <children>
      <AnchorPane prefHeight="40.0" prefWidth="378.0" style="-fx-background-color: #0AF647;">
         <children>
            <Label fx:id="lblTitle" alignment="CENTER" layoutY="9.0" prefHeight="17.0" prefWidth="378.0" text="Jugador debes ir a la ciudad " textFill="WHITE">
               <font>
                  <Font name="Century Gothic" size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Label layoutX="32.0" layoutY="55.0" text="Selecciona la ciudad a la que te desas mover">
         <font>
            <Font name="Century Gothic" size="14.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboBoxNodes" layoutX="32.0" layoutY="96.0" onAction="#changeNodeAction" prefHeight="25.0" prefWidth="315.0" promptText="Seleccione" style="-fx-background-color: transparent; -fx-border-color: #0AF647; -fx-border-width: 0px 0px 2px 0px;" />
      <Button fx:id="btnAccept" layoutX="151.0" layoutY="276.0" mnemonicParsing="false" onAction="#acceptAction" style="-fx-background-color: linear-gradient(#0AF647, #079D2E);" text="Aceptar" textFill="WHITE">
         <font>
            <Font name="Century Gothic" size="14.0" />
         </font>
      </Button>
      <Label layoutX="32.0" layoutY="138.0" text="Nodo origen:">
         <font>
            <Font name="Century Gothic" size="14.0" />
         </font>
      </Label>
      <Label layoutX="30.0" layoutY="185.0" text="Nodo destino:">
         <font>
            <Font name="Century Gothic" size="14.0" />
         </font>
      </Label>
      <Label layoutX="30.0" layoutY="234.0" text="Costo de movimiento:">
         <font>
            <Font name="Century Gothic" size="14.0" />
         </font>
      </Label>
      <Label fx:id="lblOriginNode" alignment="CENTER_RIGHT" layoutX="238.0" layoutY="139.0" prefHeight="17.0" prefWidth="109.0" text="Label">
         <font>
            <Font name="Century Gothic" size="14.0" />
         </font></Label>
      <Label fx:id="lblDestinyNode" alignment="CENTER_RIGHT" layoutX="238.0" layoutY="186.0" prefHeight="17.0" prefWidth="109.0" text="Label">
         <font>
            <Font name="Century Gothic" size="14.0" />
         </font></Label>
      <Label fx:id="lblCostMove" alignment="CENTER_RIGHT" layoutX="238.0" layoutY="235.0" prefHeight="17.0" prefWidth="109.0" text="Label">
         <font>
            <Font name="Century Gothic" size="14.0" />
         </font></Label>
   </children>
</AnchorPane>
